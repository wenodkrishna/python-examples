with open('xwrite','a') as wr:
    wr.write('vinod krishna\n')
    wr.write('k r palli\n')
    wr.write('s n puram\n')
    wr.write('andhra pradesh\n')
    wr.write('517002\n')
    print('data added succesfully\n')








