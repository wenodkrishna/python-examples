def tros():
    
    list1=[]
    c=int(input('enter how many values you want='))
    for a in range(1,c+1):
        list1.append(int(input('enter a number=')))
    print('list1=',list1)
    list1.sort()
    print('list1 ascending order=',list1)
    list1.reverse()
    print('list1 descending order=',list1)


tros()


